{
    "name": "imemento/exceptions-laravel",
    "description": "HTTP Exceptions Handler for Laravel",
    "homepage": "https://gitlab.com/imemento/composer/packages/exceptions-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "imemento/exceptions": "~5.6.0",
        "imemento/http-responses": "~5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Exceptions\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Exceptions\\Laravel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/imemento/"},
        {"packagist.org": false}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "iMemento\\Exceptions\\Laravel\\ExceptionsServiceProvider"
            ]
        }
    }
}
