{
    "name": "imemento/exceptions-laravel",
    "description": "HTTP Exceptions Handler for Laravel",
    "homepage": "https://gitlab.com/imemento/composer/packages/exceptions-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.4",
        "illuminate/contracts": "^5.4",
        "illuminate/auth": "^5.4",
        "illuminate/foundation": "^5.4",
        "illuminate/http": "^5.4",
        "illuminate/validation": "^5.4",
        "imemento/exceptions": "dev-master",
        "imemento/http-responses": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.5"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Exceptions\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Exceptions\\Laravel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
