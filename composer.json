{
    "name": "imemento/exceptions-laravel",
    "description": "HTTP Exceptions Handler for Laravel",
    "homepage": "https://gitlab.com/imemento/composer/packages/exceptions-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.0",
        "illuminate/contracts": "~5.0",
        "illuminate/auth": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/validation": "~5.0",
        "illuminate/exception": "~5.0",
        "imemento/exceptions": "dev-master",
        "imemento/http-responses": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.5"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Exceptions\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Exceptions\\Laravel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/imemento/"},
        {"packagist.org": false}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
